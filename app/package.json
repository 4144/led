{
	"name": "led",
	"version": "0.0.3",
	"main": "bin/main.js",
	"productName": "LEd",
	"scripts": {
		"start": "electron .",
		"build": "electron-builder build --win",
		"build-publish": "electron-builder build --win --publish always",
		"pack": "electron-packager . LEd --platform=win32 --arch=ia32,x64  --overwrite --out=../redist"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/deepnight/led.git"
	},
	"dependencies": {},
	"devDependencies": {
		"electron": "^9.2.1",
		"electron-builder": "^22.8.0",
		"electron-packager": "^15.1.0"
	},
	"build": {
		"appId": "com.deepnight.led",
		"asar": false,
		"directories": {
			"output": "../redist"
		},
		"win": {
			"target": "portable"
		}
	}
}
